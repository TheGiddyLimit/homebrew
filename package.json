	"license": "MIT",
	"bugs": {
		"url": "https://github.com/TheGiddyLimit/homebrew/issues"
	},
	"homepage": "https://github.com/TheGiddyLimit/homebrew#readme",
	"devDependencies": {
		"5etools-utils": "^0.13.1",
		"commander": "^9.4.1",
		"he": "^1.2.0",
		"sanitize-html": "^2.12.1"
	}
}
