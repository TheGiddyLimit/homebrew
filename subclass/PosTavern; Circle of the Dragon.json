			"languages": [
				"Common",
				"Draconic",
				"Primordial"
			],
			"cr": "5",
			"trait": [
				{
					"name": "Elemental Nature",
					"entries": [
						"The druidic dragon sleeps but doesn't require air, food, or drink."
					]
				},
				{
					"name": "Environmental Camouflage",
					"entries": [
						"While the druidic dragon remains motionless with its limbs and appendages tucked close to its body, it resembles a natural formation. A creature within 30 feet of it can discern its true nature with a successful Intelligence ({@skill Nature}) check against your druid spell save DC."
					]
				},
				{
					"name": "Empowered Attacks",
					"entries": [
						"The druidic dragon's attacks are treated as magical for the purpose of bypassing resistance and immunity to nonmagical weapons."
					]
				},
				{
					"name": "Siege Monster",
					"entries": [
						"The druidic dragon deals double damage to objects and structures."
					]
				}
			],
			"action": [
				{
					"name": "Multiattack",
					"entries": [
						"The druidic dragon makes two attacks: one with its claws and one with its tail."
					]
				},
				{
					"name": "Bite",
					"entries": [
						"{@atk mw} {@hit 8} to hit, reach 10 ft., one target. {@h}16 ({@damage 2d10 + 5}) piercing damage plus 3 ({@damage 1d6}) damage, the type of which is determined by the {@table Draconic Essence|DrdCrcDrg}."
					]
				},
				{
					"name": "Claw",
					"entries": [
						"{@atk mw} {@hit 8} to hit, reach 5 ft., one target. {@h}12 ({@damage 2d6 + 5}) slashing damage."
					]
				},
				{
					"name": "Tail",
					"entries": [
						"{@atk mw} {@hit 8} to hit, reach 15 ft., one target. {@h}14 ({@damage 2d8 + 5}) bludgeoning damage."
					]
				},
				{
					"name": "Breath Weapon {@recharge 5}",
					"entries": [
						"The druidic dragon exhales destructive energy. Each creature in the area of exhalation must make a saving throw, the type of which is determined by the {@table Draconic Essence|DrdCrcDrg}, against your druid spell save DC. A creature takes 28 ({@damage 8d6}) damage on a failed save, or half as much damage on a successful one."
					]
				},
				{
					"name": "Terrifying Reveal",
					"entries": [
						"When a creature that is unaware of the druidic dragon's presence is within 30 feet of it, the druidic dragon can use its action to reveal itself in its true nature. Each creature of its choice within 30 feet must make a Wisdom saving throw against your druid spell save DC or become {@condition frightened} for 1 minute."
					]
				}
			],
