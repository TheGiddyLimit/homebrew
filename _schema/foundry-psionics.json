{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "foundry-psionics.json",
	"version": "1.0.0",
	"type": "object",
	"properties": {
		"psionicDisciplineFocus": {
			"$ref": "util.json#/definitions/foundrySideDataGenericFeatureArray"
		},
		"psionicDisciplineActive": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string"
					},
					"source": {
						"type": "string"
					},
					"psionicName": {
						"type": "string"
					},
					"psionicSource": {
						"type": "string"
					},
					"data": {
						"$ref": "util.json#/definitions/foundryDataObject"
					},
					"flags": {
						"$ref": "util.json#/definitions/foundryFlagsObject"
					},
					"effects": {
						"$ref": "util.json#/definitions/foundryEffectsArray"
					}
				},
				"required": [
					"name",
					"source",
					"psionicName",
					"psionicSource"
				],
				"additionalProperties": false
			}
		}
	},
	"additionalProperties": false
}