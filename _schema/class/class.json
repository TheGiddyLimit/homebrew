		"proficiencies": {
			"type": "object",
			"properties": {
				"armor": {
					"type": "array",
					"items": {
						"oneOf": [
							{
								"type": "string",
								"enum": [
									"light",
									"medium",
									"heavy",
									"{@item shield|phb|shields}"
								]
							},
							{
								"type": "object",
								"properties": {
									"proficiency": {
										"type": "string",
										"enum": [
											"light",
											"medium",
											"heavy",
											"{@item shield|phb|shields}",
											"special"
										]
									},
									"full": {
										"description": "The complete text for this proficiency.",
										"type": "string"
									}
								},
								"required": [
									"proficiency",
									"full"
								]
							}
						]
					}
				},
				"weapons": {
					"type": "array",
					"items": {
						"oneOf": [
							{
								"type": "string"
							},
							{
								"type": "object",
								"properties": {
									"proficiency": {
										"type": "string"
									},
									"optional": {
										"type": "boolean"
									}
								},
								"additionalProperties": false,
								"required": [
									"proficiency",
									"optional"
								]
							}
						]
					}
				},
				"tools": {
					"type": "array",
					"items": {
						"type": "string"
					}
				},
